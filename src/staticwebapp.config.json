{
  "navigationFallback": {
      "rewrite": "index.html"
  },
  "routes": [
      {
          "route": "/login",
          "rewrite": "/.auth/login/aad",
          "allowedRoles": ["anonymous", "authenticated"]
      },
      {
          "route": "/.auth/login/github",
          "statusCode": 404
      },
      {
          "route": "/.auth/login/twitter",
          "statusCode": 404
      },
      {
          "route": "/logout",
          "redirect": "/.auth/logout",
          "allowedRoles": ["anonymous", "authenticated"]
        },
        {
          "route": "/*",
          "allowedRoles": ["authenticated"]
        }
  ],
  "responseOverrides": {
      "401": {
        "redirect": "/login",
        "statusCode": 302
      }
    }
}